<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>


  <groupId>net.benpritzker</groupId>
  <artifactId>ben-compare</artifactId>
  <version>1.1.1</version>


  <packaging>jar</packaging>

  <name>Bens Comparison Util</name>
  <description>A pure Java super simple way to compare 2 Collections that by default usually tells you what you want to know.</description>
  <url>https://github.com/bpritzker/ben-compare</url>



  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
  </properties>



  <dependencies>

    <!-- https://mvnrepository.com/artifact/org.junit.jupiter/junit-jupiter-api -->
